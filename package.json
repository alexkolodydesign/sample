{
  "name": "washington-trails",
  "version": "1.0.0",
  "description": "",
  "author": "Alex Kolody",
  "license": "ISC",
  "engines": {
    "node": "^10.14.0",
    "npm": "^6.5.0"
  },
  "scripts": {
    "dev": "nodemon -w server.js -w controllers -w data -w redirects -w helpers server.js",
    "build": "next build",
    "start": "node server.js",
    "test": "jest --watch --verbose",
    "precommit": "lint-staged",
    "postcommit": "git update-index -g",
    "heroku-postbuild": "npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index -g"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      ".*": "babel-jest"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "amdefine": "^1.0.1",
    "apicache": "^1.2.3",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "emotion": "^10.0.6",
    "emotion-server": "^10.0.6",
    "emotion-theming": "^10.0.6",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "fs": "0.0.1-security",
    "google-map-react": "^1.1.2",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "lightbox-react": "^0.3.7",
    "lodash.debounce": "^4.0.8",
    "next": "^7.0.2",
    "polished": "^2.3.3",
    "postcss-nested": "^4.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-google-maps": "^9.4.5",
    "react-html-parser": "^2.0.2",
    "react-icons": "^3.3.0",
    "react-joyride": "^2.0.2",
    "react-redux": "^6.0.0",
    "react-scroll-up": "^1.3.3",
    "react-share": "^2.4.0",
    "react-spinners": "^0.5.1",
    "recharts": "1.4.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "sanitize-html-react": "^1.13.0",
    "styled-jsx": "^3.2.0",
    "styled-jsx-plugin-postcss": "^2.0.0",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-module-resolver": "^3.1.2",
    "casual": "^1.6.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jsonlint": "^1.6.3",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.0",
    "redux-devtools": "^3.5.0",
    "redux-mock-store": "^1.5.3"
  },
  "postcss": {
    "plugins": {
      "postcss-nested": {}
    }
  }
}
